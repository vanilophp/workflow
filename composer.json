{
    "name": "vanilo/workflow",
    "description": "Workflow Engine / State Machine",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/vanilophp/workflow/issues"
    },
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "konekt/enum": "^3.1.1|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Workflow\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit" : "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Workflow\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
